<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tesla Model DataBase</title>
    <link rel="stylesheet" href="style.css">
</head>
<Body>
    

    <table >
        <tr id="trr">
            <td class="tr">Name</td>
            <td class="tr">Price</td>
            <td class="tr">Model</td>
            <td class="tr">Description</td>
            <td class="tr">Update</td>
            <td class="tr">Delete</td>
        </tr>
         <% obj.forEach(function(justOne) { %>
        <tr >
            <td id="id"><%= justOne.name %></td>
            <td id="id"><%= justOne.price %></td>
            <td id="id"><%= justOne.model %></td>
            <td id="id"><%= justOne.description %></td>
            <td id="id"><a class="update" href="update/<%= justOne._id %>">Update</a></td>
            <td id="id"><a class="delete" href="delete/<%= justOne._id %>">Delete</a></td>
        </tr>
    <% }); %>
    </table>
    <form action="/addName" method="post" id="main" >
        <input id="PrName" type="text" required name="name" placeholder="Name" class="mytext"><br><br>
        <input id="PrPrice" type="number" name="price" placeholder="Price" class="mytext"><br><br>
        <input id="PrModel" type="text" required name="model" placeholder="Model" class="mytext"><br><br>
        <input id="PrDes" type="text" name="description" placeholder="Description" class="mytext"><br><br>
        <input type="submit" class="mytext" >
    </form>

    <script src="script.js"></script>
</body>
</html>
